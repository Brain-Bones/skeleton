{
	"name": "next.skeleton.dev",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/skeletonlabs/skeleton.git"
	},
	"license": "MIT",
	"homepage": "https://skeleton.dev/",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "pnpm run --reporter append-only --color \"/dev:/\"",
		"dev:svelte": "pnpm -F @skeletonlabs/skeleton-svelte package:watch",
		"dev:react": "pnpm -F @skeletonlabs/skeleton-react package:watch",
		"dev:astro": "astro dev",
		"check": "astro check",
		"build": "pnpm -F '@skeletonlabs/*' package && astro check && astro build && pagefind",
		"preview": "astro preview",
		"astro": "astro",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@astrojs/check": "^0.5.10",
		"@astrojs/mdx": "^2.2.4",
		"@astrojs/react": "^3.1.1",
		"@astrojs/svelte": "^5.3.0",
		"@astrojs/tailwind": "^5.1.0",
		"@nanostores/persistent": "^0.9.1",
		"@skeletonlabs/skeleton": "workspace:*",
		"@skeletonlabs/skeleton-react": "workspace:*",
		"@skeletonlabs/skeleton-svelte": "workspace:*",
		"@types/react": "^18.2.74",
		"astro": "^4.5.16",
		"astro-auto-import": "^0.4.2",
		"astro-expressive-code": "^0.33.5",
		"astro-icon": "^1.1.0",
		"framer-motion": "^11.0.24",
		"lucide-react": "^0.341.0",
		"lucide-svelte": "^0.341.0",
		"nanostores": "^0.9.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"svelte": "5.0.0-next.94",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.4"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.12",
		"@types/node": "^20.12.4",
		"minimatch": "^9.0.4",
		"pagefind": "^1.1.0",
		"prettier": "^3.2.5",
		"prettier-plugin-astro": "^0.13.0",
		"vite-plugin-pagefind": "^0.2.8",
		"vite-plugin-tw-plugin-watcher": "workspace:*"
	}
}
