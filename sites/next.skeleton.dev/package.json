{
	"name": "next.skeleton.dev",
	"repository": {
		"type": "git",
		"url": "https://github.com/skeletonlabs/skeleton.git"
	},
	"license": "MIT",
	"homepage": "https://skeleton.dev/",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently \"pnpm:start\" \"pnpm:dev:svelte\"",
		"dev:svelte": "pnpm -F @skeletonlabs/skeleton-svelte package:watch",
		"start": "astro dev",
		"check": "astro check",
		"build": "pnpm -F '*svelte' build && astro check && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@astrojs/check": "^0.5.10",
		"@astrojs/mdx": "^2.2.1",
		"@astrojs/react": "^3.1.0",
		"@astrojs/svelte": "^5.2.0",
		"@astrojs/tailwind": "^5.1.0",
		"@nanostores/persistent": "^0.9.1",
		"@skeletonlabs/skeleton": "workspace:*",
		"@skeletonlabs/skeleton-react": "workspace:^",
		"@skeletonlabs/skeleton-svelte": "workspace:^",
		"@types/react": "^18.2.70",
		"astro": "^4.5.9",
		"astro-auto-import": "^0.4.2",
		"astro-expressive-code": "^0.33.5",
		"astro-icon": "^1.1.0",
		"lucide-react": "^0.341.0",
		"lucide-svelte": "^0.341.0",
		"nanostores": "^0.9.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"svelte": "5.0.0-next.79",
		"svelte-check": "^3.6.8",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.4.3"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/node": "^20.11.30",
		"minimatch": "^9.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-astro": "^0.13.0",
		"vite-plugin-tw-plugin-watcher": "workspace:*"
	}
}
