{
	"name": "@skeletonlabs/skeleton-react",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"package": "tsc --project tsconfig.package.json",
		"package:watch": "tsc --project tsconfig.package.json --watch",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"test": "vitest run",
		"test:watch": "vitest",
		"schema": "node ./scripts/generator.js"
	},
	"publishConfig": {
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"types": "./dist/lib/index.d.ts",
				"default": "./dist/lib/index.js"
			}
		},
		"typesVersions": {
			">4.0": {
				"index.d.ts": [
					"./dist/index.d.ts"
				]
			}
		}
	},
	"types": "./src/lib/index.ts",
	"exports": {
		".": {
			"types": "./dist/lib/index.d.ts",
			"default": "./dist/lib/index.js"
		},
		"./*": "./src/lib/*"
	},
	"files": [
		"./dist/**/*.js",
		"./dist/**/*.map",
		"./dist/**/*.d.ts",
		"./dist/**/*.cjs",
		"./dist/**/*.d.cts",
		"!./dist/**/*.test.*",
		"!./dist/**/schema.json"
	],
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "workspace:*",
		"@tailwindcss/forms": "^0.5.7",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^14.2.2",
		"@testing-library/user-event": "^14.5.2",
		"@types/react": "^18.2.74",
		"@types/react-dom": "^18.2.24",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"jsdom": "^24.0.0",
		"postcss": "^8.4.38",
		"react-router-dom": "^6.22.3",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.4",
		"typescript-json-schema": "^0.63.0",
		"vite": "^5.2.8",
		"vite-plugin-remix-router": "^2.0.0",
		"vite-plugin-tw-plugin-watcher": "workspace:*",
		"vitest": "^1.4.0"
	},
	"peerDependencies": {
		"framer-motion": "^11.0.24"
	}
}
