{
	"name": "@skeletonlabs/skeleton-cli",
	"description": "The CLI for Skeleton relating tooling.",
	"version": "0.0.1-next.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/skeletonlabs/skeleton",
		"directory": "packages/skeleton-cli"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs"
		},
		"./migrate": {
			"import": "./dist/commands/migrate/index.mjs"
		}
	},
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"!**/*.test.*"
	],
	"scripts": {
		"package": "unbuild",
		"package:watch": "unbuild --watch",
		"test": "vitest run",
		"test:watch": "vitest watch"
	},
	"devDependencies": {
		"@types/node": "^22.1.0",
		"@types/semver": "^7.5.8",
		"type-fest": "^4.27.0",
		"typescript": "catalog:",
		"unbuild": "^3.2.0",
		"vitest": "^2.0.5"
	},
	"license": "MIT",
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"colorette": "^2.0.20",
		"commander": "^13.0.0",
		"fast-glob": "^3.3.1",
		"latest-version": "^9.0.0",
		"package-manager-detector": "^0.2.8",
		"recast": "^0.23.9",
		"semver": "^7.6.3"
	}
}
